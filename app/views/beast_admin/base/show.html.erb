<h1>Show</h1>

<% @page.all_model_fields.each do |col| %>
  <%= @resource.send(col[:name]).to_s %>
<% end %>


<a class="" href="<%= BeastAdmin.routes.path_helper(@page.model_name, :show, @resource.id) %>">
  Show
</a>
<a class="" href="<%= BeastAdmin.routes.path_helper(@page.model_name, :edit, @resource.id) %>">
  Edit
</a>
<a class="" data-confirm="Are you sure?" data-method="delete" href="<%= BeastAdmin.routes.path_helper(@page.model_name, :delete, @resource.id) %>" rel="nofollow">
  Delete
</a>